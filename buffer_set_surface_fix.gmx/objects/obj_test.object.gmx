            <kind>1</kind>
            <string>if (surface_exists(surf)) surface_free(surf);
var w = 64;
var h = 64;
var n = w * h;
var b = buffer_create(n * 4, buffer_fixed, 1);
var a = 255 &lt;&lt; 24;
var v;
for (var i = 0; i &lt; h; i++) {
    for (var k = 0; k &lt; w; k++) {
        if ((i + k) % 8 &lt; 4) {
            var d = abs(k - i);
            if (d &lt; 4) {
                a = 127 &lt;&lt; 24;
            } else a = 255 &lt;&lt; 24;
            var c = make_color_hsv(i / h * 255, 240, 240);
            c = ((c &amp; $FF0000) &gt;&gt; 16) | ((c &amp; $0000FF) &lt;&lt; 16) | (c &amp; $00FF00)
            v = a | c;
            //v = $FFFFFFFF;
        } else v = 0;
        buffer_write(b, buffer_u32, v);
    }
}
surf = surface_create(w, h);
var t = get_timer();
buffer_set_surface(b, surf, 0, 0, 0);
elapsed = get_timer() - t;
show_debug_message("fb:" + string(buffer_set_surface_uses_fallback) + " t:" + string(elapsed));
buffer_delete(b);
</string>
